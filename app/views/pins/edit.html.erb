<h1>Edit <%= @pin.title%></h1>

<%= form_with scope: :pin, model: @pin, local: true do |form| %>

  <p>
    <%= form.label :Title %><br>
    <%= form.text_field :title%>
  </p>
  <p>
    <%= form.label :URL %><br>
    <%= form.text_field :url %>
  </p>


  <p>
    <%= form.submit %>
  </p>
<% end %>