<h1>Add A New Pin</h1>


<%= form_with scope: :pin, url: pins_path, local: true do |form| %>
  <p>
    <%= form.label :Title %><br>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :Photo_URL %><br>
    <%= form.text_field :url %>
  </p>
 

  <p>
    <%= form.submit %>
  </p>

  <%= link_to 'Back', root_path %>
<% end %>